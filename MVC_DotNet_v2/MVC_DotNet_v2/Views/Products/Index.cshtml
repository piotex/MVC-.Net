@model List<MVC_DotNet_v2.Models.DbModels.DbModel_Products>

<h1>Products</h1>
<Reference Include="netstandard" />

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Quantity</th>
            <th scope="col">Type</th>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
        </tr>
    </thead>
    <tbody>
        @{
            MVC_DotNet_v2.Models.DbModels.DbModel_Products a = new MVC_DotNet_v2.Models.DbModels.DbModel_Products() { id = -1 };
            Html.RenderPartial("Product", a);
        }
        @for (int i = 0; i < Model.Count(); i++)
        {
            Html.RenderPartial("Product", Model[i]);
        }
    </tbody>
</table>
    






