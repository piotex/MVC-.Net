@model List<MVC_DotNet_v2.Models.DbModels.DbModelJoin_Products>

<h1>Products</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Available</th>
            <th scope="col">Price</th>
            <th scope="col"><div style="width:300px"></div></th>
            <th scope="col">Quantity </th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < (Model as List<MVC_DotNet_v2.Models.DbModels.DbModelJoin_Products>).Count; i++)
        {
            Html.RenderPartial("UserProduct", Model[i]);
        }
    </tbody>
</table>







